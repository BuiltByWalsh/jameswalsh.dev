{
  "name": "portfolio",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=18.16"
  },
  "packageManager": "pnpm@8.6.5",
  "scripts": {
    "dev": "next dev --port=3001",
    "dev:force": "rm -rf .next && pnpm dev",
    "type-check": "tsc --noEmit --incremental",
    "build": "next build",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint": "next lint",
    "prepare": "panda codegen && husky install",
    "start": "next start"
  },
  "dependencies": {
    "@heroicons/react": "^2.0.18",
    "@types/node": "20.4.5",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "contentlayer": "^0.3.4",
    "date-fns": "^2.30.0",
    "eslint": "8.46.0",
    "eslint-config-next": "13.4.7",
    "next": "13.4.12",
    "next-contentlayer": "^0.3.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "shiki": "^0.14.3",
    "typescript": "5.1.6"
  },
  "devDependencies": {
    "@pandacss/dev": "0.9.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.8",
    "rehype": "^12.0.1",
    "rehype-autolink-headings": "^6.1.1",
    "rehype-pretty-code": "^0.10.0",
    "rehype-slug": "^5.1.0",
    "remark-gfm": "^3.0.1"
